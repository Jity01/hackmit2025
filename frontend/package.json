{
  "name": "knowledgebase-electron-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "watch:electron": "tsc -w -p electron/tsconfig.json",
    "build:electron": "tsc -p electron/tsconfig.json",
    "dev": "concurrently -k \"npm:watch:electron\" \"vite --config vite.config.ts --port 5173\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "vite build --config vite.config.ts && npm run build:electron",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.20",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "electron": "^31.3.0",
    "lucide-react": "^0.452.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.5"
  }
}
